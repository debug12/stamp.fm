				</ul><br>
				</div>
				<p style="color:#FFFFFF;text-align:center;">Have fun, and may the best artist win!</p>
			  </div>
			</div>
		<%}

	}

%>

		
		
		
		
		



<script>
$("document").ready(function(){
	$('.close').click(function(){
		$(this).parent().parent().removeClass("unhidden");
	});
	
	$('.modalbutton').click(function(event){
		var str = $(this).attr("id");
		$('#'+str.substring(0, str.length - 1)).addClass("unhidden");
	});
	$('#preview').click(function(){
      if($('#previe').hasClass('unhidden') == true){
      $('#previe').click(function(){
        $('#previe').removeClass('unhidden');
      })
    } 
    })
	
});

</script>