<% layout('layout') -%>
<% stylesheet('/stylesheets/main.css') -%>
<% script('/scripts/main.js') -%>

<form id="newArtist" method = "POST" action = "/save">
    Name: <input type="text" name="name"><br>
    Song Title: <input type="text" name="songTitle">
    <input type="submit" value="submit">
</form>

<div id="wrapper">
    <div class="center">
        <ul id="centerTabs">
            <li id="auditionTab">Audition</li>
            <li id="tournamentTab">Tournament</li>
            <li id="profileTab">Profile</li>
        </ul>
    </div>
    <div class="matchup">
        <form id="artist1" class="vote_song">
            <iframe width="420" height="236" src="http://www.youtube.com/embed/5LNTXE9e5ok?list=PL_rKzeF6K2knMJVKDKG_UMdU2P77Ho09U?rel=0" frameborder="0" allowfullscreen></iframe>
            <div class="voteButton">
                <input type="button" id="vote1" value=" Vote " class="voteButton">
            </div>
            <div class="shareButton" id="fb2">
